<userDefinedSQL><![CDATA[CREATE OR REPLACE VIEW W_EMPLOYEE_PROMOTION
AS 
SELECT /*+ PARALLEL(4) */     
    employee_name                 ,
    employee_surname              ,
    employee_patronymic           ,
    position_now                  ,
    position_old                  ,
    position_change_date          ,
    salary                        ,
    bonus                         ,
    work_length                   
    FROM DW_DATA.t_dw_employees]]></userDefinedSQL>
